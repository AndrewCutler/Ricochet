{
	"manifest_version": 2,
	"name": "Ricochet",
	"description": "Automatically re-direct away from blacklisted sites.",
	"version": "1.0",
	"background": {
		"scripts": ["script.js"]
	},
	"browser_action": {
		"default_icon": "assets/icon.png",
		"default_popup": "popup/popup.html"
	},
	"permissions": ["activeTab", "tabs", "storage", "webNavigation"]
}
